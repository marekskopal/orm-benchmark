{
    "name": "marekskopal/orm-benchmark",
    "type": "project",
    "description": "ORM Benchmark",
    "license": "MIT",
    "authors": [
        {
            "name": "Marek Skopal",
            "homepage": "https://www.marekskopal.cz"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-pdo": "*",
        "symfony/console": "^7.2",
        "marekskopal/orm": "^0.9",
        "nette/utils": "^4.0",
        "cycle/orm": "^2.9",
        "cycle/database": "^2.11",
        "cycle/annotated": "^4.2",
        "cycle/schema-builder": "^2.11"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpstan/extension-installer": "^1.4",
        "slevomat/coding-standard": "^8.15",
        "phpstan/phpstan-strict-rules": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MarekSkopal\\ORMBenchmark\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.4"
        },
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
